{
  "name": "thalia",
  "private": true,
  "version": "4.0.2",
  "description": "Frostickle's server",
  "main": "index.js",
  "bin": {
    "thalia": "./server/thalia.js"
  },
  "dependencies": {
    "amdefine": "^1.0.1",
    "formidable": "^3.5.0",
    "handlebars": "^4.7.8",
    "http-proxy": "^1.18.1",
    "lodash": "^4.17.21",
    "mime": "^2.6.0",
    "minimist": "^1.2.8",
    "requirejs": "^2.3.6",
    "sass": "^1.65.1",
    "sequelize": "^6.32.1",
    "socket.io": "^4.7.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@types/d3": "^5.16.4",
    "@types/d3-collection": "^1.0.10",
    "@types/d3-selection": "^3.0.5",
    "@types/d3-selection-multi": "^1.0.10",
    "@types/d3-voronoi": "^1.1.9",
    "@types/datatables.net": "^1.10.24",
    "@types/formidable": "^3.4.1",
    "@types/http-proxy": "^1.17.11",
    "@types/jest": "^26.0.24",
    "@types/jquery": "^3.5.16",
    "@types/lodash": "^4.14.197",
    "@types/mime": "^2.0.3",
    "@types/node": "^18.17.5",
    "@types/nodemailer": "^6.4.9",
    "@types/puppeteer": "^5.4.7",
    "@types/sequelize": "^4.28.15",
    "@types/showdown": "^1.9.4",
    "@types/validator": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "10.2.6",
    "browser-sync": "^2.29.3",
    "cssnano": "5.0.5",
    "d3": "^5.16.0",
    "d3-selection": "^2.0.0",
    "d3-selection-multi": "^1.0.1",
    "d3-transition": "^3.0.1",
    "del": "6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.33.1",
    "gulp": "4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "2.6.1",
    "gulp-flatmap": "1.0.2",
    "gulp-jshint": "2.1.0",
    "gulp-postcss": "9.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "5.0.0",
    "gulp-terser": "2.0.1",
    "jshint": "2.13.0",
    "jshint-stylish": "2.2.1",
    "lazypipe": "1.0.2",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.27",
    "readline-sync": "^1.4.10",
    "sequelize-cli": "^6.6.1",
    "typescript": "^4.9.5",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node server/thalia.js",
    "build": "./build.sh",
    "watch": "./watch.sh",
    "test": "./test.sh",
    "buildServer": "cd server; tsc",
    "watchTests": "cd test; tsc --preserveWatchOutput --watch",
    "develop": "./develop.sh"
  },
  "nodemonConfig": {
    "exec": "node server/thalia.js",
    "verbose": true,
    "watch": [
      "./server/thalia.js",
      "./websites/**/config.js",
      "./websites/**/config/*.js",
      "./websites/**/db_bootstrap.js"
    ],
    "ignore": [
      "node_modules/*",
      "dist/*",
      "src/*",
      "views/*",
      "public/*",
      "*.ts",
      "tsconfig.json",
      "gulpfile.js"
    ],
    "delay": "500"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error"
      ],
      "no-useless-constructor": "off",
      "@typescript-eslint/no-useless-constructor": [
        "error"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "es6": true
    },
    "ignorePatterns": [
      "gulpfile.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/david-ma/Thalia.git"
  },
  "author": "David Ma aka @Frostickle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/david-ma/Thalia/issues"
  },
  "homepage": "https://github.com/david-ma/Thalia#readme"
}
