    "yargs": "^16.0.3"
  },
  "resolutions": {
    "browser-sync/localtunnel/yargs": "13.3.2",
    "gulp-sass/node-sass": "4.14.1"
  },
  "engines": {
    "node": ">=10.12"
  },
  "scripts": {
    "start": "sudo ./start.sh 80",
    "build": "./build.sh",
    "watch": "./watch.sh",
    "test": "./test.sh",
    "buildServer": "cd server; tsc",
    "watchTests": "cd test; tsc --preserveWatchOutput --watch",
    "cleanup": "ps -ef | grep 'jest\\|puppeteer\\|tsc --preserveWatchOutput --watch\\|nodemon\\|gulp\\|server/thalia.js\\|watchTests' | grep -v grep | awk '{print $2}' | xargs -t kill -9",
    "badCleanupDoNotUse": "ps -ef | grep 'jest\\|puppeteer\\|tsc --preserveWatchOutput --watch\\|nodemon\\|gulp\\|server/thalia.js\\|watchTests' | grep -v grep | bash -c 'tee >(awk \"{print $2}\" | xargs -t kill -9)'",
    "develop": "./develop.sh"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "./server/thalia.js",
      "./websites/**/config.js",
      "./websites/**/config/*.js",
      "./websites/**/db_bootstrap.js"
    ],
    "ignore": [
      "node_modules/*",
      "dist/*",
      "src/*",
      "views/*",
      "public/*",
      "*.ts",
      "tsconfig.json",
      "gulpfile.js"
    ],
    "delay": "500"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error"
      ],
      "no-useless-constructor": "off",
      "@typescript-eslint/no-useless-constructor": [
        "error"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "es6": true
    },
    "ignorePatterns": [
      "gulpfile.js"
    ]
  },
  "standardx": {
    "globals": [
      "d3",
      "$",
      "JQuery",
      "DataTables",
      "showdown",
      "io"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "ignore": [
      "server/index.ts",
      "server/moduleShim.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/david-ma/Thalia.git"
  },
  "author": "David Ma aka @Frostickle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/david-ma/Thalia/issues"
  },
  "homepage": "https://github.com/david-ma/Thalia#readme"
}
