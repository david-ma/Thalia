{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "strict": false,
    "declaration": true,
    "removeComments": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,

    "stripInternal": true,
    "noEmitHelpers": true,

    "skipLibCheck": true
  },
  "files": [
    "helpers.ts"
  ]
}