    "compilerOptions": {
        "module":"AMD",
        "moduleResolution": "node",
        "skipLibCheck": false,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "outFile": "thalia.js",
        "target": "ES2020"
    },
    "files":[
        "moduleShim.ts",
        "requestHandlers.ts",
        "router.ts",
        "server.ts",
        "database.ts",
        "socket.ts",
        "index.ts"
    ]
}