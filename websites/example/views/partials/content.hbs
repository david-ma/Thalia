<main class="content">
  <div class="container">
    <section class="features">
      <h2>Features</h2>
      <div class="feature-grid">
        {{#each features}}
        <div class="feature">
          <h3>{{title}}</h3>
          <p>{{description}}</p>
        </div>
        {{/each}}
      </div>
    </section>
  </div>
</main> 